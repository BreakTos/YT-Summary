{
    "name": "Yt Summary",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "Nothing much",
    "author": "Parth",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": ["activeTab"],
    "content_scripts": [
        {
            "matches": ["*://www.youtube.com/watch*"],   
            "js": ["content.js"]
        }
    ]
}
